<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="253.0" prefWidth="376.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.sda.JavaFXWeather.rootController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="174.0" prefWidth="376.0" VBox.vgrow="ALWAYS">
      <children>
            <Label fx:id="labelCity" layoutX="20.0" layoutY="75.0" text="Podaj miasto" textAlignment="CENTER">
               <effect>
                  <Blend />
               </effect>
            </Label>
            <TextField fx:id="textfieldCity" layoutX="118.0" layoutY="70.0" prefHeight="31.0" prefWidth="135.0" />
            <Label layoutX="15.0" layoutY="145.0" text="Temperatura:">
               <effect>
                  <Blend />
               </effect>
            </Label>
            <Label layoutX="15.0" layoutY="186.0" text="Temperatura odczuwalna:">
               <effect>
                  <Blend />
               </effect>
            </Label>
            <Label fx:id="labelTempC" layoutX="218.0" layoutY="145.0" text="">
               <effect>
                  <Blend />
               </effect>
            </Label>
            <Label fx:id="labelTempFeel" layoutX="218.0" layoutY="186.0" text="">
               <effect>
                  <Blend />
               </effect>
            </Label>
            <Button fx:id="buttonSearch" layoutX="268.0" layoutY="70.0" mnemonicParsing="false" onAction="#setCity" prefHeight="31.0" prefWidth="75.0" text="Generuj">
               <effect>
                  <Blend />
               </effect>
            </Button>
            <ImageView fx:id="weatherIcon" fitHeight="61.0" fitWidth="75.0" layoutX="268.0" layoutY="145.0" pickOnBounds="true" preserveRatio="true" />
      </children>
    </AnchorPane>
  </children>
</VBox>
